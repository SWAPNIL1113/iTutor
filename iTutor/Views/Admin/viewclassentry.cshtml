@model IEnumerable<iTutor.Models.Class>

@{
    ViewBag.Title = "viewclassentry";
    Layout = "~/Views/Admin.cshtml";
}

<div class="wrapper">
    <div class="card">
        <center>
            <div class="card-header">
                <h3 class="card-title">Student Register Entry</h3>
            </div>
        </center>

        <div class="card-body">
            <table class="table table-bordered">
                <tr>
                    <th>
                        Standard
                    </th>
                    <th>Update</th>
                    <th>Dalete</th>
                </tr>

                @foreach (var item1 in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item1.std)
                        </td>
                        <td>@Html.ActionLink("Edit", "EditStd", new { id = item1.id }, new {@class="btn btn-info"})
                        </td>
                        <td>
                            @Html.ActionLink("Delete", "DeleteStd", new { id = item1.id },new {@class="btn btn-danger"})
                        </td>

                    </tr>
                }

            </table>
        </div>
    </div>
</div>